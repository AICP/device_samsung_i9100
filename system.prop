#
# system.prop for i9100
#

rild.libpath=/system/lib/libsecril-shim.so
rild.libargs=-d /dev/ttyS0 --

# By default, primary storage is physical
ro.vold.primary_physical=1

# Atlas-Service
config.disable_atlas=true

ro.ril.mqanelements=5

# Hardware Info
ro.device.chipset=Exynos 4210 Dual
ro.device.cpu=Dual-core 1.2 GHz Cortex-A9
ro.device.gpu=Mali-400MP4
ro.device.rear_cam=8 MP
ro.device.front_cam=2.0 MP
ro.device.screen_res=480x800

# ART
dalvik.vm.dex2oat-swap=false
dalvik.vm.dex2oat-Xmx=256m

# Skip screen-off Animation
ro.egl.nofadeoff=1

# GSM Signal
ro.telephony.ril.config=datacallapn,signalstrength
